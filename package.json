{
  "name": "uule-converter",
  "version": "0.0.0",
  "repository": "github:etler/uule-converter",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "types/index.d.ts",
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "prepublishOnly": "npm run lint && npm run clean && npm run build",
    "prepare": "husky && ts-patch install -s",
    "clean": "rm -rf dist/ types/",
    "build": "tsc && mkdir -p dist/proto && scripts/generate-proto > dist/proto/uule-proto.js",
    "lint": "scripts/lint",
    "fix": "scripts/generate-proto-types > src/proto/uule-proto.d.ts && scripts/lint --fix",
    "test": "exit 0"
  },
  "dependencies": {
    "protobufjs": "^7.4.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "eslint": "~9.21.0",
    "eslint-plugin-prettier": "~5.2.3",
    "eslint-plugin-tsdoc": "~0.4.0",
    "globals": "^16.0.0",
    "husky": "~9.1.7",
    "lint-staged": "~15.4.3",
    "prettier": "~3.5.2",
    "protobufjs-cli": "^1.1.3",
    "ts-patch": "~3.3.0",
    "typescript": "~5.7.3",
    "typescript-eslint": "^8.25.0",
    "typescript-transform-paths": "~3.5.3"
  }
}
